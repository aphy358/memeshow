.hh-header {
  position: fixed;
  top: 0;
  width: 100%;
  /* background-color: #57a7fe; */
  background: #FAFAFA;
  z-index: 99;
}
.hh-title{
  flex:1;
  /* display: flex; */
  justify-content: flex-start;
  font-size: 30rpx;
  color: #000000;
  text-align: center;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.title-bar {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.hh-nav-right{
  width: 100px;
  height: 100%
}
.hh-nav-back{
  width: 100px;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

