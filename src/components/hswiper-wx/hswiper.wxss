/*滑动样式*/
@import "./hSwiper/swiperTemplate.wxss";

.hSwiper {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  overflow: hidden;
}

.hSwiperCon {
	height: 100%;
	/* will-change: transform; */
	position: absolute;
  overflow: hidden;
}
.item {
	position:relative;
	float: left;
  box-sizing:border-box;
  overflow: hidden;
}
