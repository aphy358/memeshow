/*滑动样式*/
@import "./hSwiper/swiperTemplate.wxss";

.hSwiper {
  box-sizing: border-box;
  overflow: hidden;
}

.hSwiperCon {
	position: fixed;
	height: 100%;
	will-change: transform;
  overflow: hidden;
}
.hSwiper-item {
  position: absolute;
  top: 0;
  left: 0;
  box-sizing:border-box;
  overflow: hidden;
}

.hSwiper-mask{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
	height: 100%;
  background: opacity;
  z-index: 9999;
}
