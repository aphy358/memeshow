/* pages/search/index.wxss */
.search-list-container {
  width: 100%;
  padding: 0 30rpx;
}
.search-list {
  width: 690rpx;
  margin-top: 30rpx;
}
.search-list .item {
  float: left;
  min-width: 124rpx;
  height: 58rpx;
  line-height: 58rpx;
  text-align: center;
  background: #f5f5f5;
  border-radius: 29rpx;
  padding: 0 20rpx;
  margin-right: 20rpx;
  margin-top: 20rpx;
  color: #333333;
  font-size: 26rpx;
}
.search-list::after {
  content: '';
  display: block;
  clear: both;
}
.title {
  font-size: 26rpx;
  color: #333333;
}