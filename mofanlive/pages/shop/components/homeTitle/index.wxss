.title-input {
    display: flex;
    position: relative;
    align-items: center;
    width: 510rpx;
    height: 64rpx;
    margin-left: 32rpx;
    background-color: #fff;
    border-radius: 32rpx;
}

.contain-title {
    width: 510rpx;
}

.title-input-placeholder {
    width: 198rpx;
    height: 40px;
    line-height: 40rpx;
    font-size: 28rpx;
    color: #999999;
    margin-left: 72rpx;
    text-align: left;
}

.title-search-icon {
    margin-left: 24rpx;
    width: 32rpx;
    height: 32rpx;
    background-image: url('https://pinduoduoimg.yangkeduo.com/wxappimg/2019-11-10/9276c3619c542cdd392fc443954a80c8.png');
    background-size: contain;
    background-repeat: no-repeat;
}